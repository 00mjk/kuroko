import math
import random

def calcPi():
    let inside = 0
    let total = 0
    for i = 0; i < 1000000; i++:
        let x = random.random()
        let y = random.random()
        if x * x + y * y <= 1.0:
            inside += 1
        total += 1
    print('pi = ', 4.0 * (inside / total))

calcPi()

